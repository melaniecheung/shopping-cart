ktop/shopping-cart/frontend/src/index.js",["100"],[],"import React from 'react';\nimport ReactDOM from 'react-dom/client';\nimport './index.css';\nimport App from './App';\nimport { BrowserRouter } from 'react-router-dom';\nimport CartProvider from './components/CartContext';\n\nconst root = ReactDOM.createRoot(document.getElementById('root'));\nroot.render(\n    <CartProvider>\n      <App />\n    </CartProvider>\n);","/Users/melaniecheung/Desktop/shopping-cart/frontend/src/App.js",["101","102"],[],"import Navbar from './components/Navbar';\nimport HomePage from './pages/HomePage';\nimport ProductPage from './pages/ProductPage';\nimport Cart from './components/Cart';\nimport { BrowserRouter as Router, Routes, Route } from 'react-router-dom';\nimport AboutPage from './pages/AboutPage';\nimport SignInButton from './components/SignInButton';\nimport SignInPage from './pages/SignInPage';\nimport CartPage from './pages/CartPage';\n\nconst App = () => {\n  return (\n    <div>\n      <Router>\n        {/* <SignInButton/> */}\n        {/* <Cart /> */}\n        <Navbar />\n        <Routes>\n          <Route path='/shopping-cart' element={<HomePage />} />\n          <Route path='/shopping-cart/products' element={<ProductPage />} />\n          <Route path='/shopping-cart/about' element={<AboutPage />}/>\n          <Route path='/shopping-cart/signin' element={<SignInPage />}/>\n          <Route path='/shopping-cart/cart' element={<CartPage />}/>\n        </Routes>\n      </Router>\n    </div>\n  );\n}\n\nexport default App;\n",[],"/Users/melaniecheung/Desktop/shopping-cart/frontend/src/components/CartContext.jsx",[],[],"/Users/melaniecheung/Desktop/shopping-cart/frontend/src/components/Navbar.jsx",["103"],[],"import React from 'react';\nimport { NavLink } from 'react-router-dom';\nimport Cart from './Cart';\nimport { RiShoppingCartLine } from 'react-icons/ri';\n\nconst Navbar = () => {\n  return (\n    <div className='bg-[#F8F8F8] flex items-center h-16 mx-auto px-8 text-black'>\n      <div>\n        <NavLink to='/shopping-cart'>\n          <h1 className='mr-4 text-2xl font-bold hover:cursor-pointer hover:text-[#8F00FF] transition'>Store</h1>\n        </NavLink>\n      </div>\n\n    <div className='flex-grow flex justify-center'>\n      <ul className='flex'>\n        <li className='p-4 hover:cursor-pointer font-semibold hover:text-[#8F00FF] transition'>\n          <NavLink to='/shopping-cart/products'>Products</NavLink>\n        </li>\n        <li className='p-4 hover:cursor-pointer font-semibold hover:text-[#8F00FF] transition'>\n          <NavLink to='/shopping-cart/about'>About</NavLink>\n        </li>\n      </ul>\n    </div>\n\n    <div className='flex justify-end'>\n      <ul className='flex'>\n      {localStorage.getItem('auth-token')?\n        <button className='mr-6 font-semibold hover:cursor-pointer hover:text-[#8F00FF] transition' onClick={() => {localStorage.removeItem('auth-token');window.location.replace('/')}}>Logout</button>:        \n        <li className='mr-6 font-semibold hover:cursor-pointer hover:text-[#8F00FF] transition'>\n          <NavLink to='/shopping-cart/signin'>Sign In</NavLink>\n        </li>}\n        <li>\n          <NavLink to='/shopping-cart/cart'>\n            <RiShoppingCartLine className='hover:text-[#8F00FF] text-2xl'/>\n          </NavLink>\n        </li>\n      </ul>\n    </div>\n    </div>\n  );\n};\n\nexport default Navbar;","/Users/melaniecheung/Desktop/shopping-cart/frontend/src/components/SignInButton.jsx",[],[],"/Users/melaniecheung/Desktop/shopping-cart/frontend/src/components/Cart.jsx",[],[],"/Users/melaniecheung/Desktop/shopping-cart/frontend/src/pages/AboutPage.jsx",[],[],"/Users/melaniecheung/Desktop/shopping-cart/frontend/src/pages/ProductPage.jsx",["104","105"],[],"import ProductList from '../components/ProductList';\nimport Footer from '../components/Footer';\nimport { CartContext } from '../components/CartContext';\nimport React, { useContext } from 'react'\n\nconst Productpage = () => {\n  return (\n    <div className='bg-[#F8F8F8]'>\n      <ProductList />\n      <Footer />\n    </div>\n  );\n};\n\nexport default Productpage","/Users/melaniecheung/Desktop/shopping-cart/frontend/src/pages/SignInPage.jsx",["106","107","108","109"],[],"/Users/melaniecheung/Desktop/shopping-cart/frontend/src/pages/HomePage.jsx",[],[],"/Users/melaniecheung/Desktop/shopping-cart/frontend/src/components/CartCard.jsx",[],[],"/Users/melaniecheung/Desktop/shopping-cart/frontend/src/components/ProductList.jsx",["110","111","112","113"],[],"import React from 'react'\nimport { useState, useEffect } from 'react';\nimport axios from 'axios';\n\nimport Product from '../components/Product';\n\nfunction ProductList() {\n  const [products, setProducts] = useState([]);\n\n  useEffect(() => {\n      fetch('http://localhost:4000/allproducts')\n      .then((response) => response.json())\n      .then((data) => setProducts(data))\n  }, [])\n\n  const filterClothing = products.filter(item => {\n    return item.category === \"men's clothing\" || item.category === \"women's clothing\"\n  })\n  \n  const filterElectronics = products.filter(item => {\n    return item.category === \"electronics\"\n  })\n  \n  const filterjewelery = products.filter(item => {\n    return item.category === \"jewelery\"\n  })\n  \n  return (\n    <div className='py-16'>\n      <div className='container mx-auto'>\n        <div className='grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-3 gap-[3rem] max-w-sm mx-auto md:max-w-none'>\n          {products.map(product => (\n            <Product product={product} key={product.id} />\n            ))}\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default ProductList;\n\n\n\n\n\n// useEffect(() => {\n//   axios.get('https://fakestoreapi.com/products')\n//     .then(response => {\n//       setProducts(response.data);\n//     })\n//     .catch(error => {\n//       console.error('Error fetching products:', error);\n//     });\n// }, []);\n","/User