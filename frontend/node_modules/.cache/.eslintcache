e} style={{ width: '200px', height: 'auto' }} alt=\"\" /> \n                      </div>\n\n                      <div className='w-full flex flex-col pl-5 pt-5'> \n\n                        <div className='font-semibold flex justify-between mb-2'>\n                          <p>{e.name}</p>\n                          <p>${e.new_price*cartItems[e.id]}</p>\n                        </div>\n\n                      {/* <div className='flex gap-x-2 h-[36px] text-sm mt-2'>\n                        <p>${e.new_price}</p>\n                      </div> */}\n\n                      {/* <div>\n                        <p>{e.category}</p>\n                      </div> */}\n\n                        <div className='flex gap-x-2 h-[36px] text-sm mt-2'>\n                          <div className='flex flex-1 max-w-[100px] items-center h-full border text-primary font-medium'>\n                              <div onClick={() => removeFromCart(e.id)} className='flex-1 flex justify-center items-center cursor-pointer transition hover:text-black text-gray-500'>\n                                  <IoMdRemove />\n                              </div>\n                              <div className='h-full flex justify-center items-center px-2'>{cartItems[e.id]}</div>\n                              <div onClick={() => addToCart(e.id)} className='flex-1 h-full flex justify-center items-center cursor-pointer transition hover:text-black text-gray-500'>\n                                  <IoMdAdd />\n                              </div>\n                          </div>\n                          {/* item price */}\n                          {/* <div className='flex-1 flex justify-end'>${parseFloat(new_price).toFixed(2)}</div> */}\n                        </div>\n\n                        <div className=\"mt-auto pb-5\">\n                          {/* <p>${e.new_price*cartItems[e.id]}</p> */}\n                          <div onClick={() => removeAllFromCart(e.id)} className='text-xl hover:cursor-pointer'>\n                            <PiTrash className='text-gray-500 transition hover:text-black'/>\n                          </div>\n                        </div>\n\n                      </div>\n                    </div>\n                    <hr />\n                  </div>;\n        }\n        return null\n        })}\n    </div>\n  )\n}\n\nexport default CartItems","/Users/melaniecheung/Desktop/shopping-cart/frontend/src/components/CartTotal.jsx",[],[],{"ruleId":"120","severity":1,"message":"121","line":5,"column":10,"nodeType":"122","messageId":"123","endLine":5,"endColumn":23},{"ruleId":"120","severity":1,"message":"124","line":4,"column":8,"nodeType":"122","messageId":"123","endLine":4,"endColumn":12},{"ruleId":"120","severity":1,"message":"125","line":7,"column":8,"nodeType":"122","messageId":"123","endLine":7,"endColumn":20},{"ruleId":"120","severity":1,"message":"124","line":3,"column":8,"nodeType":"122","messageId":"123","endLine":3,"endColumn":12},{"ruleId":"120","severity":1,"message":"126","line":3,"column":10,"nodeType":"122","messageId":"123","endLine":3,"endColumn":21},{"ruleId":"120","severity":1,"message":"127","line":4,"column":17,"nodeType":"122","messageId":"123","endLine":4,"endColumn":27},{"ruleId":"128","severity":1,"message":"129","line":60,"column":9,"nodeType":"130","endLine":60,"endColumn":93},{"ruleId":"128","severity":1,"message":"129","line":91,"column":15,"nodeType":"130","endLine":91,"endColumn":92},{"ruleId":"128","severity":1,"message":"129","line":96,"column":40,"nodeType":"130","endLine":96,"endColumn":109},{"ruleId":"128","severity":1,"message":"129","line":99,"column":42,"nodeType":"130","endLine":99,"endColumn":111},{"ruleId":"120","severity":1,"message":"131","line":3,"column":8,"nodeType":"122","messageId":"123","endLine":3,"endColumn":13},{"ruleId":"120","severity":1,"message":"132","line":16,"column":9,"nodeType":"122","messageId":"123","endLine":16,"endColumn":23},{"ruleId":"120","severity":1,"message":"133","line":20,"